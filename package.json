{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/fastify-webpack/**/*"
    ],
    "ignoredByWatcher": [
      "test/fixtures/**/*"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "fastify-plugin": "^3.0.1",
    "memfs": "^3.4.1",
    "mime-types": "^2.1.35",
    "negotiator": "^0.6.3",
    "roarr": "^7.10.0",
    "serialize-error": "^8.1.0",
    "typed-emitter": "^2.1.0"
  },
  "description": "A Fastify plugin for serving files emitted by Webpack with Hot Module Replacement (HMR).",
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "ava": "^3.15.0",
    "compression-webpack-plugin": "^9.2.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.11.0",
    "eslint-config-canonical": "^33.0.1",
    "fastify": "^3.27.4",
    "gitdown": "^3.1.4",
    "got": "^11.8.3",
    "husky": "^7.0.2",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.7",
    "sinon": "^11.1.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "fastify",
    "webpack"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "fastify-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/fastify-webpack"
  },
  "scripts": {
    "build": "rm -fr ./dist && tsc && webpack",
    "generate-readme": "gitdown ./.README/README.md --output-file ./README.md",
    "lint": "eslint ./src ./test && tsc --noEmit",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "typings": "./dist/src/index.d.ts",
  "version": "1.0.0"
}
